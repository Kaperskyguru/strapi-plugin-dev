{
  "kind": "collectionType",
  "info": {
    "tableName": "linkr-links",
    "singularName": "linkr-link",
    "pluralName": "linkr-links",
    "displayName": "Links"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "min": 1,
      "required": true,
      "configurable": false
    },
    "target": {
      "type": "string",
      "min": 1,
      "required": true,
      "configurable": false
    },
    "redirection": {
      "type": "enumeration",
      "enum": [
        "temporary_redirect_307",
        "found_302",
        "moved_permanently_301"
      ],
      "default": "temporary_redirect_307",
      "configurable": false
    },
    "link": {
      "type": "string",
      "required": true,
      "unique": true,
      "column": {
        "unique": true
      },
      "configurable": false
    },
    "shouldEnableTracking": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "paramentForwarding": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "isSponsored": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "isNoFollow": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "notes": {
      "type": "text",
      "configurable": false
    }
  }
}